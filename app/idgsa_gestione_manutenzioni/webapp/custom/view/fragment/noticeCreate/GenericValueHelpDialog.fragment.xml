<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>selectItem}"
		class="sapUiPopupWithPadding"
		items="{/results}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			title="{nameComplete}"
			description="{user}"
			/>
	</SelectDialog>
  
    
</core:FragmentDefinition>